{
    "train_transforms": [
        {
            "package": "transforms",
            "name": "CenterSegment",
            "argv": {
                "size": 16
            }
        },
        {
            "name": "Resize",
            "argv": {
                "size": 256
            }
        },
        {
            "name": "CenterCrop",
            "argv": {
                "size": [224, 224]
            }
        },
        {
            "package": "torchstream.transforms",
            "name": "ToTensor"
        },
        {
            "package": "torchstream.transforms",
            "name": "Normalize" ,
            "argv": {
                "mean": [0.485, 0.456, 0.406],
                "std": [0.229, 0.224, 0.225]
            }
        }
    ],

    "train_dataset": {
        "package": "datasets",
        "name": "SomethingSomethingV1Tiny",
        "argv": {
            "train": true
        }
    },

    "val_transforms": [
        {
            "package": "transforms",
            "name": "CenterSegment",
            "argv": {
                "size": 16
            }
        },
        {
            "name": "Resize",
            "argv": {
                "size": 256
            }
        },
        {
            "name": "CenterCrop",
            "argv": {
                "size": [224, 224]
            }
        },
        {
            "package": "torchstream.transforms",
            "name": "ToTensor"  
        },
        {
            "package": "torchstream.transforms",
            "name": "Normalize" ,
            "argv": {
                "mean": [0.485, 0.456, 0.406],
                "std": [0.229, 0.224, 0.225]
            }
        }
    ],

    "val_dataset": {
        "package": "datasets",
        "name": "SomethingSomethingV1Tiny",
        "argv": {
            "train": false
        }
    },

    "train_loader":{
        "batch_size": 16,
        "num_workers": 64,
        "shuffle": true,
        "pin_memory": true,
        "drop_last": true
    },

    "val_loader":{
        "batch_size": 16,
        "num_workers": 64,
        "shuffle": false,
        "pin_memory": true,
        "drop_last": false
    },

    "model": {
        "package": "kfs",
        "name": "Wrapper",
        "argv": {
            "cls_num": 4,
            "input_size": [16, 224, 224]
        }
    },

    "criterion": {
        "name": "CrossEntropyLoss"
    },

    "optimizer": {
        "name": "SGD",
        "argv": {
            "params": "model_specified",
            "lr": 0.001,
            "momentum": 0.9,
            "weight_decay": 0.0001
        }
    },

    "lr_scheduler": {
        "name": "StepLR",
        "argv": {
            "step_size": 10
        }
    },

    "train": {
        "epochs": 20,
        "backup": {
            "dir_path": "checkpoints/KFS/sthsthv1tiny",
            "pth_name": "KFS.sthsthv1tiny.rgb.s8.f16.no-aug.b16.e20"
        },
        "resume": {
            "dir_path": "checkpoints/KFS/sthsthv1tiny",
            "pth_name": "KFS.sthsthv1tiny.rgb.s8.f16.no-aug.b16.e20"
        },
        "classifier": {
            "pretrained": {
                "dir_path": "checkpoints/TSM/sthsthv1tiny",
                "pth_name": "TSM.sthsthv1tiny.rgb.s8.no-aug.b32.e20.best"
            },
            "freeze": false
        }
    }

}
