{
    "test_dataset": {
        "name": "HMDB51",
        "argv": {
            "train": false,
            "transforms": [
                {
                    "package": "transforms",
                    "name": "RandomFrameSampler",
                    "argv": {
                        "size": 8
                    }      
                },
                {
                    "name": "Resize",
                    "argv": {
                        "size": [256, 256]
                    }      
                },
                {
                    "name": "CenterCrop",
                    "argv": {
                        "size": [224, 224]
                    }      
                },
                {
                    "package": "torchstream.transforms",
                    "name": "ToTensor"  
                },
                {
                    "package": "torchstream.transforms",
                    "name": "Normalize" ,
                    "argv": {
                        "mean": [0.485, 0.456, 0.406],
                        "std": [0.229, 0.224, 0.225]
                    }
                }
            ]
        }
    },

    "test_loader":{
        "batch_size": 64,
        "num_workers": 32,
        "shuffle": false,
        "pin_memory": true,
        "drop_last": false
    },

    "model": {
        "package": "torchstream.models",
        "name": "TSN",
        "argv": {
            "cls_num": 51,
            "input_size": [8, 224, 224],
            "base_model": "resnet50",
            "dropout": 0.8,
            "partial_bn": true,
            "use_softmax": false
        }
    },

    "criterion": {
        "name": "CrossEntropyLoss"
    }

}
